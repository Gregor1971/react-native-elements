{
  "name": "native",
  "preset": "react-native",
  "moduleNameMapper": {
    "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
  },
  "testPathIgnorePatterns": ["<rootDir>/node_modules", ".web.test.js"],
  "globals": {
    "__DEV__": true
  },
  "setupTestFrameworkScriptFile": "<rootDir>/testSetup.js",
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "<rootDir>/node_modules/(?!(react-native|react-native-vector-icons|enzyme-styleguidist-sample-parser)/)"
  ]
}
